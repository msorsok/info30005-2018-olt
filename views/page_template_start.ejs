<body>
<div class="page">
    <div class = "header">
        <div class="logo">

            <div class = "logoImage">
                <img src="/res/logoImage.png" alt="logo image" height="30" width="30">
            </div>
            <div class = "logoText">
                <p>
                    One Last Time
                </p>
            </div>
        </div>
        <div class = "user">
            <div class = "userImage">
                <img id = "headerProfilePic" src="/profilepic" alt="User Profile Picture" >
            </div>
            <div class = "userName">
                <p>
                    <% if (typeof firstName == 'string' && firstName) { %>
                        <%-firstName%>
                    <% } %>
                </p>
            </div>
            <div class = "logout">
                <img src = "/res/logout.png" alt = "logout button" height = "20" width = "20">
            </div>
        </div>

    </div>

    <div class = "navigation">
        <div class = "homeNav">
            <p>Home</p>
        </div>
        <div class = "accountNav">
            <p>Account</p>
        </div>
        <div class = "releaseNav">
            <p>
                Release Capsules
            </p>
        </div>
        <div class = "createNav">
            <p>Create Capsule</p>
        </div>
    </div>
    <div class="content">